{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "noEmitOnError": true,
    "noImplicitAny": true,
    "types": [
      "jasmine",
      "lodash"
    ],
    "paths": {
      "@aribaui/core": [
        "./lib/core/index"
      ],
      "@aribaui/components": [
        "./lib/components/index"
      ],
      "@aribaui/metaui": [
        "./lib/metaui/index"
      ],


      "@angular/*": ["../node_modules/@angular/*"]
    }
  }
}
