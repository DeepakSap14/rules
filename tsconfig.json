{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "target": "es5",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "jasmine",
      "object-path"
    ],
    "paths": {
      "@aribaui/core": [
        "dist/@aribaui/core"
      ],
      "@aribaui/core/*": [
        "dist/@aribaui/core/*"
      ],
      "@aribaui/components": [
        "dist/@aribaui/components"
      ],
      "@aribaui/components/*": [
        "dist/@aribaui/components/*"
      ],
      "resources": [
        "dist/resources"
      ],
      "resources/*": [
        "dist/resources/*"
      ],
      "metaui": [
        "dist/metaui"
      ],
      "metaui/*": [
        "dist/metaui/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false
  }
}