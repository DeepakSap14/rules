{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "target": "es5",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "jasmine",
      "lodash"
    ],
    "paths": {
      "@aribaui/core": [
        "./src/lib/core/index"
      ],
      "@aribaui/components": [
        "./src/lib/components/index"
      ],
      "@aribaui/metaui": [
        "./src/lib/metaui/index"
      ]
    }
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false
  }
}
