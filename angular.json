{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "rules": {
      "root": "libs/rules",
      "sourceRoot": "libs/rules/src",
      "projectType": "library",
      "prefix": "ngx-meta",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/rules/tsconfig.lib.json",
            "project": "libs/rules/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "libs/rules/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/rules/src/test.ts",
            "tsConfig": "libs/rules/tsconfig.spec.json",
            "karmaConfig": "libs/rules/karma.conf.js",
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/quill/dist/quill.js"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "libs/rules/src/lib/resources",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/rules/tsconfig.lib.json",
              "libs/rules/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "playground": {
      "root": "apps/playground/",
      "sourceRoot": "apps/playground/src",
      "projectType": "application",
      "prefix": "ngx-meta",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "showCircularDependencies": false,
            "outputPath": "dist/apps/playground",
            "index": "apps/playground/src/index.html",
            "main": "apps/playground/src/main.ts",
            "polyfills": "apps/playground/src/polyfills.ts",
            "tsConfig": "apps/playground/tsconfig.app.json",
            "assets": [
              "apps/playground/src/favicon.ico",
              "apps/playground/src/assets",
              {
                "glob": "**/*",
                "input": "libs/rules/src/lib/resources",
                "output": "/assets"
              }
            ],
            "styles": [
              "libs/rules/src/lib/resources/themes/_normalize.scss",
              "node_modules/prismjs/themes/prism.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "libs/rules/src/lib/resources/fonts/sap-ariba-icon-fonts/sap-ariba-icon-fonts.css",
              "libs/rules/src/lib/resources/fonts/sap-icon-fonts/sap-icon-fonts.css",
              "node_modules/primeicons/primeicons.css",
              "libs/rules/src/lib/resources/themes/ariba/theme.scss",
              "libs/rules/src/lib/resources/styles/aribaui.scss",
              "apps/playground/src/styles.scss"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/quill/dist/quill.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/playground/src/environments/environment.ts",
                  "with": "apps/playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "playground:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/playground/src/test.ts",
            "polyfills": "apps/playground/src/polyfills.ts",
            "tsConfig": "apps/playground/tsconfig.spec.json",
            "karmaConfig": "apps/playground/karma.conf.js",
            "styles": [
              "apps/playground/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/playground/src/favicon.ico",
              "apps/playground/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/playground/tsconfig.app.json",
              "apps/playground/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "metaui-evolution": {
      "root": "apps/metaui-evolution/",
      "sourceRoot": "apps/metaui-evolution/src",
      "projectType": "application",
      "prefix": "ngx-meta",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "spec": false
        },
        "@schematics/angular:class": {
          "spec": false
        },
        "@schematics/angular:directive": {
          "spec": false
        },
        "@schematics/angular:guard": {
          "spec": false
        },
        "@schematics/angular:module": {
          "spec": false
        },
        "@schematics/angular:pipe": {
          "spec": false
        },
        "@schematics/angular:service": {
          "spec": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "showCircularDependencies": false,
            "outputPath": "dist/apps/metaui-evolution",
            "index": "apps/metaui-evolution/src/index.html",
            "main": "apps/metaui-evolution/src/main.ts",
            "polyfills": "apps/metaui-evolution/src/polyfills.ts",
            "tsConfig": "apps/metaui-evolution/tsconfig.app.json",
            "assets": [
              "apps/metaui-evolution/src/favicon.ico",
              "apps/metaui-evolution/src/assets"
            ],
            "styles": [
              "libs/rules/src/lib/resources/themes/_normalize.scss",
              "node_modules/prismjs/themes/prism.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "libs/rules/src/lib/resources/fonts/sap-ariba-icon-fonts/sap-ariba-icon-fonts.css",
              "libs/rules/src/lib/resources/fonts/sap-icon-fonts/sap-icon-fonts.css",
              "node_modules/primeicons/primeicons.css",
              "libs/rules/src/lib/resources/themes/ariba/theme.scss",
              "libs/rules/src/lib/resources/styles/aribaui.scss",
              "apps/metaui-evolution/src/styles.scss"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/quill/dist/quill.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/metaui-evolution/src/environments/environment.ts",
                  "with": "apps/metaui-evolution/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "metaui-evolution:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "metaui-evolution:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "metaui-evolution:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/metaui-evolution/src/test.ts",
            "polyfills": "apps/metaui-evolution/src/polyfills.ts",
            "tsConfig": "apps/metaui-evolution/tsconfig.spec.json",
            "karmaConfig": "apps/metaui-evolution/karma.conf.js",
            "styles": [
              "apps/metaui-evolution/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/metaui-evolution/src/favicon.ico",
              "apps/metaui-evolution/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/metaui-evolution/tsconfig.app.json",
              "apps/metaui-evolution/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "defaultProject": "rules"
}
