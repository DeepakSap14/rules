{
  "name": "aribaui",
  "description": "Ariba UI core module providing basic set of API and components.",
  "homepage": "https://github.com/SAP/angular-metaui.git",
  "version": "6.0.0",
  "license": "Apache-2.0",
  "keywords": [
    "angular",
    "ariba",
    "sap",
    "metaui",
    "declarative",
    "components",
    "model",
    "fieldpath",
    "rules"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ",
    "build:aot": "ng build --prod --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngh": "ngh",
    "oss-sys": "java -jar lib/meta-ui-parser.jar --gen --sys ./src/lib/metaui/core",
    "oss-user": "java -jar lib/meta-ui-parser.jar --gen --user ./src/lib/metaui/core ./src/playground/rules",
    "build.resources": "rm -Rf ./dist/@aribaui/resources && cp -R packages/resources/src/ ./dist/@aribaui/resources && ./node_modules/.bin/scss-bundle -e  dist/@aribaui/resources/styles/aribaui.scss -d dist/@aribaui/resources/styles/aribaui.css && cp -rf  packages/resources/package.json ./dist/@aribaui/resources/.",
    "build.packages": "ng build core && ng build components && npm run build.resources && build metaui --prod",
    "build.packages.prod": "ng build core --prod && ng build components --prod && npm run build.resources && build metaui --prod",
    "test.packages": "echo Testing core && ng test core  --watch=false && echo Testing components && ng test components --watch=false && echo Testing metaui && ng test metaui --watch=false",
    "lint.packages": "echo Linting core && ng lint core  && echo Linting components && ng lint components && echo Linting metaui && ng lint metaui ",
    "release.pa": "standard-version --release-as patch",
    "release.mi": "standard-version --release-as minor",
    "release.ma": "standard-version --release-as major"
  },
  "private": true,
